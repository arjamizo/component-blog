{
  "name": "Post",
  "base": "PersistedModel",
  "indexes": {},
  "plural": "post",
  "strict": false,
  "idInjection": false,
  "forceId": true,
  "options": {
    "validateUpsert": true
  },
  "replaceOnPUT": true,
  "properties": {
    "postTitle": {
      "type": "String",
      "required": true
    },
    "postType": {
      "type": "String",
      "required": true
    },
    "postText": {
      "type": "String",
      "required": false
    },
    "createdAt": {
      "type": "Date",
      "required": true
    },
    "modifiedAt": {
      "type": "Date"
    },
    "isEnabled": {
      "type": "Boolean",
      "required": true,
      "default": true
    },
    "deletedAt": {
      "type": "Date",
      "default": null
    },
    "isReported": {
      "type": "Boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "userCreated": {
      "type": "belongsTo",
      "model": "WeflexUser",
      "foreignKey": "createdBy",
      "options": {
        "validate": true
      }
    },
    "userModified": {
      "type": "belongsTo",
      "model": "WeflexUser",
      "foreignKey": "modifiedBy",
      "options": {
        "validate": true
      }
    },
    "userDeleted": {
      "type": "belongsTo",
      "model": "WeflexUser",
      "foreignKey": "deletedBy",
      "options": {
        "validate": true
      }
    },
    "venue": {
      "type": "belongsTo",
      "model": "Venue",
      "foreignKey": "venueId",
      "options": {
        "validate": false
      }
    },
    "media": {
      "type": "hasMany",
      "model": "PostMedia",
      "foreignKey": "postId",
      "options": {
        "validate": false
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ]
}
